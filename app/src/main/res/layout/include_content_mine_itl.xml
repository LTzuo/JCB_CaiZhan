<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/ll_content"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:fitsSystemWindows="true"
    android:orientation="vertical">

    <LinearLayout
        android:id="@+id/lin1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/white"
        android:orientation="horizontal"
        android:padding="10dp">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:drawableLeft="@drawable/bird"
            android:drawablePadding="8dp"
            android:drawableTint="@color/cornflowerblue"
            android:gravity="center"
            android:text="开奖结果"
            android:textColor="@color/black_light"
            android:textSize="@dimen/minefragment_lift_text_size" />

        <TextView
            style="@style/font_style"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:drawableRight="@drawable/more"
            android:gravity="end|center_vertical"
            android:textColor="@color/grey1"
            android:textSize="@dimen/minefragment_right_text_size" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/white"
        android:orientation="horizontal"
        android:padding="10dp">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:drawableLeft="@drawable/bird"
            android:drawablePadding="8dp"
            android:drawableTint="@color/cornflowerblue"
            android:gravity="center"
            android:text="发起众筹"
            android:textColor="@color/black_light"
            android:textSize="@dimen/minefragment_lift_text_size" />

        <TextView
            style="@style/font_style"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:drawableRight="@drawable/more"
            android:gravity="end|center_vertical"
            android:textColor="@color/grey1"
            android:textSize="@dimen/minefragment_right_text_size" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/white"
        android:orientation="horizontal"
        android:padding="10dp">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:drawableLeft="@drawable/bird"
            android:drawablePadding="8dp"
            android:drawableTint="@color/blueviolet"
            android:gravity="center"
            android:text="未出票"
            android:textColor="@color/black_light"
            android:textSize="@dimen/minefragment_lift_text_size" />


        <TextView
            style="@style/font_style"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:drawableRight="@drawable/more"
            android:gravity="end|center_vertical"
            android:text="0单未出"
            android:textColor="@color/grey1"
            android:textSize="@dimen/minefragment_right_text_size" />

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="15dp"
        android:background="@color/white"
        android:orientation="horizontal"
        android:padding="10dp">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:drawableLeft="@drawable/bird"
            android:drawablePadding="8dp"
            android:drawableTint="@color/palegreen"
            android:gravity="center"
            android:text="派奖"
            android:textColor="@color/black_light"
            android:textSize="@dimen/minefragment_lift_text_size" />


        <TextView
            style="@style/font_style"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:drawableRight="@drawable/more"
            android:gravity="end|center_vertical"
            android:text="0单未派奖"
            android:textColor="@color/grey1"
            android:textSize="@dimen/minefragment_right_text_size" />

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/white"
        android:orientation="horizontal"
        android:padding="10dp">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:drawableLeft="@drawable/bird"
            android:drawablePadding="8dp"
            android:gravity="center"
            android:text="委托兑奖"
            android:textColor="@color/black_light"
            android:textSize="@dimen/minefragment_lift_text_size" />


        <TextView
            style="@style/font_style"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:drawableRight="@drawable/more"
            android:gravity="end|center_vertical"
            android:textColor="@color/grey1"
            android:textSize="@dimen/minefragment_right_text_size" />

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/white"
        android:orientation="horizontal"
        android:padding="10dp">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:drawableLeft="@drawable/bird"
            android:drawablePadding="8dp"
            android:gravity="center"
            android:text="上门取票"
            android:textColor="@color/black_light"
            android:textSize="@dimen/minefragment_lift_text_size" />


        <TextView
            style="@style/font_style"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:drawableRight="@drawable/more"
            android:gravity="end|center_vertical"
            android:textColor="@color/grey1"
            android:textSize="@dimen/minefragment_right_text_size" />

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="15dp"
        android:background="@color/white"
        android:orientation="horizontal"
        android:padding="10dp">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:drawableLeft="@drawable/bird"
            android:drawablePadding="8dp"
            android:drawableTint="@color/gold"
            android:gravity="center"
            android:text="提现"
            android:textColor="@color/black_light"
            android:textSize="@dimen/minefragment_lift_text_size" />


        <TextView
