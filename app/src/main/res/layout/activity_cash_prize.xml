<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.cjkj.jcb_caizhan.modul.Personal_Center.cash_prize.CashPrizeActivity">

    <include layout="@layout/toolbar_custom"/>

    <com.cjkj.jcb_caizhan.widget.VerticalDrawerLayout
        android:id="@+id/mVerticalDrawerLayout"
        android:layout_marginTop="@dimen/customToolbarHight"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <FrameLayout
            android:id="@+id/content_layout"
            android:layout_width="match_parent"
            android:layout_height="match_parent"/>

        <GridView
            android:id="@+id/mCustomGridView"
            android:layout_marginTop="@dimen/customToolbarHight"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:paddingLeft="15dp"
            android:paddingRight="15dp"
            android:background="@color/white"
            android:numColumns="3"/>

    </com.cjkj.jcb_caizhan.widget.VerticalDrawerLayout>



</FrameLayout>
